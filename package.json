{
  "name": "website",
  "version": "0.0.1",
  "description": "performance, accessibility, sustainability, security, and ethics",
  "scripts": {
    "prettier": "npx prettier . -w",
    "eslint": "npx eslint",
    "csslint": "npx stylelint src/**/*.css --fix",
    "test": "node tests/index.test.js",
    "static": "cp src/robots.txt dist",
    "start": "parcel && npm run static",
    "build": "parcel build --dist-dir dist && npm run static",
    "ibm": "npx achecker ./src",
    "publish": "gh-pages -d dist"
  },
  "source": [
    "./src/index.html",
    "./src/test-card-submit.html"
  ],
  "keywords": [
    "performance",
    "accessibility",
    "sustainability",
    "security",
    "and",
    "ethics"
  ],
  "author": "JP and Dan",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "accessibility-checker": "^3.1.74",
    "eslint": "^9.8.0",
    "gh-pages": "^6.1.1",
    "globals": "^15.9.0",
    "parcel": "^2.12.0",
    "prettier": "^3.3.3",
    "puppeteer": "^22.15.0",
    "stylelint": "^16.8.1",
    "stylelint-config-standard": "^36.0.1"
  },
  "type": "module",
  "targets": {
    "main": {
      "optimize": true
    }
  }
}
